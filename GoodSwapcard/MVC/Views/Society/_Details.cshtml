@model MVC.Models.Views.AddListSociety

<div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
        <div class="table-responsive">
            <div class="modal-header">
                <h5 class="modal-title">Society</h5>
                <button type="button" class="close" data-dismiss="modal">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                <dl class="jumbotron">
                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutSociety.addSociety.SocietyName) : @Html.DisplayFor(model => model.ajoutSociety.addSociety.SocietyName)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutSociety.addSociety.SocietyDesc) : @Html.DisplayFor(model => model.ajoutSociety.addSociety.SocietyDesc)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutSociety.addSociety.Img) : @Html.DisplayFor(model => model.ajoutSociety.addSociety.Img)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutSociety.addSociety.WebSite) : @Html.DisplayFor(model => model.ajoutSociety.addSociety.WebSite)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutSociety.addSociety.Phone) : @Html.DisplayFor(model => model.ajoutSociety.addSociety.Phone)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutSociety.addSociety.Street) : @Html.DisplayFor(model => model.ajoutSociety.addSociety.Street)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutSociety.addSociety.Number) : @Html.DisplayFor(model => model.ajoutSociety.addSociety.Number)
                    </dt>

                    <dt>
                        Ville : @Html.DisplayFor(model => model.ajoutSociety.addSociety.LLoc.LocalityName)
                    </dt>

                    <dt>
                        Propriétaire : @Html.DisplayFor(model => model.ajoutSociety.addSociety.Boss.LastName)
                    </dt>
                </dl>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Back to List</button>
            </div>
        </div>
    </div>
</div>

