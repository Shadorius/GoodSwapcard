@model MVC.Models.Views.AddListUser

<div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
        <div class="table-responsive">
            <div class="modal-header">
                <h5 class="modal-title">Utilisateur</h5>
                <button type="button" class="close" data-dismiss="modal">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <dl class="jumbotron">
                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutUser.Utilisateur.LastName) : @Html.DisplayFor(model => model.ajoutUser.Utilisateur.LastName)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutUser.Utilisateur.FirstName) : @Html.DisplayFor(model => model.ajoutUser.Utilisateur.FirstName)
                    </dt>
                    
                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutUser.Utilisateur.Email) : @Html.DisplayFor(model => model.ajoutUser.Utilisateur.Email)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutUser.Utilisateur.Phone) : @Html.DisplayFor(model => model.ajoutUser.Utilisateur.Phone)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutUser.Utilisateur.Birthdate) : @Html.DisplayFor(model => model.ajoutUser.Utilisateur.Birthdate)
                    </dt>

                    <dt>
                        @Html.DisplayNameFor(model => model.ajoutUser.Utilisateur.statut.StatutName) : @Html.DisplayFor(model => model.ajoutUser.Utilisateur.statut.StatutName)
                    </dt>

                </dl>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Back to List</button>
            </div>
        </div>
    </div>
</div>

